{
  "name": "sura/framework",
  "description": "System parts",
  "keywords": ["support"],
  "license": "MIT",
  "type": "library",
  "require": {
    "php": "^8.1",
    "ext-iconv": "*",
    "ext-zlib": "*"
  },
  "authors": [
    {
      "name": "Semen Alekseev",
      "email": "semyon492@ya.ru"
    }
  ],
  "require-dev": {
    "phpunit/phpunit": "^9.5.20 || ^10.0.0",
    "php-coveralls/php-coveralls": "^2.1",
    "phpstan/phpstan": "^1.5",
    "jetbrains/phpstorm-attributes": "1.0",
    "squizlabs/php_codesniffer": "^3.6"
  },
  "autoload": {
    "files": [
      "src/helpers.php"
    ],
    "psr-4": {
      "Sura\\": "src/"
    }
